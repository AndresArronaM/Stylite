{
    "MediaQuery":{
        "xs":"480px",
        "s":"640px",
        "m":"960px",
        "lg":"1024px",
        "xl":"1200px"
    },
    "flexbox":{
        "direction":{
            "r":"row",
            "c":"column",
            "rr":"row-reverse",
            "cr":"column-reverse"
        },
        "wrap":{
            "w":"wrap",
            "wr":"wrap-reverse",
            "nw":"nowrap"
        },
        "orden":{
            "start":"0",
            "end":"9"
        },
        "grow":{
            "start":"0",
            "end":"9"
        },
        "basis":{
            "start":"0",
            "end":"9"
        },
        "shrink":{
            "start":"0",
            "end":"9"
        }
    },
    "grid":{
        "template":{
            "columns":{
                "valor":"12",
                "width":"1fr"
            },
            "row":{
                "valor":"12",
                "height":"1fr"
            }
        },
        "auto":{
            "columns":"auto",
            "flow":{
                "r":"row",
                "c":"column",
                "d":"dense",
                "rd":"row dense",
                "cd":"column dense"
            },
            "rows":"auto"
        },
        "column":{
            "start":"1",
            "end":"12"
        },
        "row":{
            "start":"1",
            "end":"12"
        }
    },
    "boxalign":{
        "align":{
            "content":{
                "fs":"flex-start",
                "fe":"flex-end",
                "c":"center",
                "s":"start",
                "e":"end",
                "st":"stretch",
                "sb":"space-between",
                "sa":"space-around",
                "se":"space-evenly"
            },
            "items":{
                "st":"stretch",
                "fs":"flex-start",
                "fe":"flex-end",
                "s":"start",
                "e":"end",
                "ss":"self-start",
                "se":"self-end",
                "c":"center",
                "b":"baseline"
            },
            "self":{
                "a":"auto",
                "fs":"flex-start",
                "fe":"flex-end",
                "c":"center",
                "b":"baseline",
                "st":"stretch",
                "s":"start",
                "e":"end"
            }         
        },
        "justify":{
            "content":{
                "fs":"flex-start",
                "fe":"flex-end",
                "c":"center",
                "sb":"space-between",
                "sa":"space-around",
                "se":"space-evenly",
                "s":"start",
                "e":"end",
                "st":"stretch"
            },
            "items":{
                "st":"stretch",
                "s":"start",
                "e":"end",
                "c":"center"
            },
            "self":{
                "c":"center",
                "st":"stretch",
                "s":"start",
                "e":"end"
            }         
        },
        "gap":{
            "column":{
                "start":"1fr",
                "end":"12fr"
            },
            "row":{
                "start":"1fr",
                "end":"12fr"
            }
        }
    }
}